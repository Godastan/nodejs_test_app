#!/usr/bin/env node

var fixtures = require('pow-mongoose-fixtures');
var path = require('path');
var config = require('../config');
var mongoose = require('mongoose');

mongoose.connect(config.get('mongoose:uri'));

fixtures.load({
    Organizations: [
        {
            name: 'ИП Синникова',
            type: 'IP'
        },{
            name: 'ЧТУП КсанаСвит',
            type: 'ChTUP'
        }
    ]
}, mongoose, () => mongoose.connection.close());
